<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="category.CategoriesController">
   <center>
      <TreeView fx:id="treeView" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <BorderPane prefHeight="157.0" prefWidth="478.0" BorderPane.alignment="CENTER">
         <center>
            <TextArea fx:id="Log" editable="false" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <Label prefHeight="17.0" prefWidth="483.0" text="Log: " textAlignment="CENTER" BorderPane.alignment="CENTER" />
         </top>
         <left>
            <BorderPane BorderPane.alignment="CENTER">
               <center>
                  <VBox BorderPane.alignment="CENTER">
                     <children>
                        <TextField fx:id="categoryName" />
                        <Button fx:id="insertCategory" mnemonicParsing="false" onAction="#insertCategoryPress" prefHeight="25.0" prefWidth="149.0" text="Einfügen" />
                     </children>
                  </VBox>
               </center>
               <top>
                  <VBox BorderPane.alignment="CENTER">
                     <children>
                        <Label text="Bitte trage einen Namen " />
                        <Label text="für die Kategorie ein: " />
                     </children>
                  </VBox>
               </top>
            </BorderPane>
         </left>
      </BorderPane>
   </bottom>
</BorderPane>
